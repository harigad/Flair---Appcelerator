function print(e,t){var n=Ti.UI.createView({height:"auto",layout:"vertical"});t.add(n);var r=Ti.UI.createView({height:"25",width:"100%",left:0,top:0,backgroundColor:"#999"}),i=Ti.UI.createLabel({left:10,top:5,bottom:5,width:"auto",height:"auto",color:"#fff",text:e.vicinity,font:{fontSize:11}});r.add(i),n.add(r),_lat=parseFloat(e.lat),_lng=parseFloat(e.lng);if(!_lat||!_lng)_lat=.02,_lng=.08;mapView=Ti.Map.createView({mapType:Ti.Map.STANDARD_TYPE,width:320,height:100,top:0,region:{latitude:_lat,longitude:_lng,latitudeDelta:.001,longitudeDelta:.001},animate:!0}),n.add(mapView);if(e.feed){var s=require("ui/common/feed/FeedView"),o=new s(e.feed);n.add(o)}}function print_cast(e,t){var n=Ti.UI.createView({height:"auto",layout:"vertical"});t.add(n);for(var r=0;r<e.cast.length;r++){var i=Ti.UI.createView({height:"auto",width:"100%",left:0,top:0,layout:"vertical"}),s=Ti.UI.createImageView({image:e.cast[r].photo_big,width:"100%"}),o=Ti.UI.createView({height:"auto",width:"100%",left:0,top:0,backgroundColor:"#eee",layout:"vertical"}),u=Ti.UI.createLabel({left:10,top:10,width:"auto",color:"#999",text:e.cast[r].name,font:{fontSize:16}}),a=Ti.UI.createLabel({left:10,bottom:10,width:"auto",color:"#2179ca",shadowColor:"#aaa",shadowOffset:{x:1,y:1},text:e.cast[r].role_name,font:{fontSize:25}});o.add(u),o.add(a),i.add(s),i.add(o),n.add(i)}}function loadData(e,t,n){var r=this;if(e.lat&&e.lng){print(e,t);return}var i="http://flair.me/search.php",s={};s.type="search",s.searchMode="place",s.pid=e.pid,s.accessToken=Ti.Facebook.getAccessToken();var o=Ti.Network.createHTTPClient({onload:function(r){Ti.API.debug("loaded data for Place -> "+e.pid);var i=JSON.parse(this.responseText);print(i,t),print_cast(i,n)},onerror:function(t){Ti.API.error("error loading data for Place -> "+e.pid),Ti.API.error(t.error)},timeout:5e3});o.open("POST",i),o.send(s)}var _lat,_lng;exports.init=function(e){var t=Ti.UI.createWindow({title:name,barColor:"#333",backgroundColor:"#eee"}),n=Ti.UI.createView({height:"auto",width:"auto",backgroundColor:"#999",bottom:0,layout:"horizontal"}),r=Ti.UI.createScrollView({contentWidth:"auto",contentHeight:"auto",showVerticalScrollIndicator:!1,showHorizontalScrollIndicator:!1,width:320,top:0}),i=Ti.UI.createScrollView({contentWidth:"auto",contentHeight:"auto",showVerticalScrollIndicator:!1,showHorizontalScrollIndicator:!1,width:320,top:0,visible:!1});t.add(r),t.add(i);var s=Ti.UI.createView({left:5,top:5,right:2.5,bottom:5,width:152.5,height:"40",backgroundColor:"#cecece",borderRadius:4});s.addEventListener("click",function(e){this.setBackgroundColor("#cecece"),this.otherTab.setBackgroundColor("#aaa"),i.hide(),r.show()}),n.add(s);var o=Ti.UI.createView({left:2.5,top:5,right:5,bottom:5,width:152.5,height:"40",backgroundColor:"#aaa",borderRadius:4,otherTab:s});return s.otherTab=o,o.addEventListener("click",function(e){this.setBackgroundColor("#cecece"),this.otherTab.setBackgroundColor("#aaa"),r.hide(),i.show()}),n.add(o),t.add(n),loadData(e,r,i),t}