var main;exports.init=function(){main=Ti.UI.createWindow({backgroundColor:"#fff",title:"Checkin",barColor:"#aaa",barImage:"images/home/shadow.png"});var e=Ti.UI.createView({width:"100%",height:"177",backgroundImage:"images/checkin/bg.png",top:0}),t=Ti.UI.createTextField({value:"",width:220,top:50,hintText:"Plate #",autocapitalization:Ti.UI.TEXT_AUTOCAPITALIZATION_ALL,returnKeyType:Ti.UI.RETURNKEY_NEXT,borderStyle:Ti.UI.INPUT_BORDERSTYLE_NONE,font:{fontSize:50}});return e.add(t),main.add(e),main}