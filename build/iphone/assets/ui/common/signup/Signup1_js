function process(e){var t="http://flair.me/carkey/secure.php?page=signup2",n="plate="+plateField.value+"&state=TX";n=n+"&accessToken="+Ti.Facebook.getAccessToken();var r=Ti.Network.createHTTPClient({onload:function(t){var n=JSON.parse(this.responseText);if(!n.status)var r=Ti.UI.createAlertDialog({message:n.error,ok:"Okay",title:"Oops!"}).show();else{var i=require("ui/common/signup/Signup2");i.init(e,n),main.close()}},onerror:function(e){Ti.API.debug(e.error),alert("error")},timeout:5e3});r.open("POST",t),r.send(n)}var plateField,passwordField,main;exports.init=function(e){main=Ti.UI.createWindow({backgroundImage:"images/signup/bg.png"});var t=Ti.UI.createImageView({image:"images/logo.png",bottom:20});plateField=Ti.UI.createTextField({value:"",left:10,right:10,height:40,hintText:"Car Licence Plate #",autocapitalization:Ti.UI.TEXT_AUTOCAPITALIZATION_ALL,returnKeyType:Ti.UI.RETURNKEY_NEXT,borderStyle:Ti.UI.INPUT_BORDERSTYLE_NONE});var n=Ti.UI.createTextField({value:"",height:40,hintText:"Zipcode",left:10,right:10,borderStyle:Ti.UI.INPUT_BORDERSTYLE_NONE,returnKeyType:Ti.UI.RETURNKEY_DONE}),r=Ti.UI.createButton({title:"Create My Car Key",top:10,width:280,height:40});r.addEventListener("singletap",function(){process(e)}),plateField.addEventListener("return",function(e){passwordField.focus()}),n.addEventListener("return",function(e){});var i=Ti.UI.createView({width:"auto",height:"auto",layout:"vertical",top:50});i.add(t);var s=Ti.UI.createView({width:"280",height:"auto",layout:"vertical",backgroundColor:"#ffffff",borderRadius:4});s.add(plateField),s.add(Ti.UI.createView({width:"100%",height:"1",backgroundColor:"#cecece"})),s.add(n),i.add(s),i.add(r),main.add(i);var o=Ti.UI.createActivityIndicator({color:"green",font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",top:"30%",left:"50%",height:"auto",width:"auto"});main.open()}