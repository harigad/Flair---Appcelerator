function process(e){var t="http://flair.me/carkey/secure.php?page=ccprocess",n="amt="+amtField.value+"&ccField="+ccField+"&expField="+expField;n=n+"&accessToken="+Ti.Facebook.getAccessToken();var r=Ti.Network.createHTTPClient({onload:function(t){var n=JSON.parse(this.responseText);if(!n.status)var r=Ti.UI.createAlertDialog({message:n.error,ok:"Okay",title:"Oops!"}).show();else main.close(),e()},onerror:function(e){Ti.API.debug(e.error)},timeout:5e3});r.open("POST",t),r.send(n)}var main,amtField,ccField,expField;exports.init=function(e,t){main=Ti.UI.createWindow({backgroundImage:"images/signup/bg.png"});var n=Ti.UI.createImageView({image:"images/48/"+t.logo}),r=Ti.UI.createLabel({width:"auto",height:"auto",color:"#fff",bottom:10,text:t.model,font:{fontSize:40}});amtField=Ti.UI.createTextField({value:"",left:10,right:10,height:40,hintText:"$0.00",returnKeyType:Ti.UI.RETURNKEY_NEXT,borderStyle:Ti.UI.INPUT_BORDERSTYLE_NONE}),ccField=Ti.UI.createTextField({value:"",left:10,right:10,height:40,hintText:"credit card number",returnKeyType:Ti.UI.RETURNKEY_NEXT,borderStyle:Ti.UI.INPUT_BORDERSTYLE_NONE}),expField=Ti.UI.createTextField({value:"",left:10,right:10,height:40,hintText:"MMYY",borderStyle:Ti.UI.INPUT_BORDERSTYLE_NONE,returnKeyType:Ti.UI.RETURNKEY_DONE});var i=Ti.UI.createButton({title:"Verify My Ownership",top:10,width:280,height:40});i.addEventListener("click",function(){process(e)}),amtField.addEventListener("return",function(e){}),ccField.addEventListener("return",function(e){}),expField.addEventListener("return",function(e){});var s=Ti.UI.createView({width:"auto",height:"auto",layout:"vertical",top:50});s.add(n),s.add(r);var o=Ti.UI.createView({width:"280",height:"auto",layout:"vertical",backgroundColor:"#ffffff",borderRadius:4});o.add(amtField);var u=Ti.UI.createView({width:300,height:1,backgroundColor:"#cecece"});o.add(u),o.add(ccField);var a=Ti.UI.createView({width:300,height:1,top:1,backgroundColor:"#cecece"});o.add(a),o.add(expField),s.add(o),s.add(i),main.add(s),main.open()}