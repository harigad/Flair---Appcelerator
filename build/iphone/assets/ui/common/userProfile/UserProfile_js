function cleanWindow(e){if(e.children){Ti.API.info("Children: "+e.children);for(var t=e.children.length;t>0;t--){var n=e.children[t-1];e.remove(n),n=null}}e=null}var profileView=require("ui/common/userProfile/UserProfileView");exports.init=function(e,t,n){var r=Ti.UI.createWindow({barColor:"#333",backgroundColor:"#eee"});return r.add(profileView.init(e,t,n)),r.addEventListener("hide",function(e){cleanWindow(r)}),r}