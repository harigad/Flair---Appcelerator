function _createThumb(e,t){var n=Ti.UI.createView({left:10,right:10,top:10,width:70,height:70,backgroundImage:"images/flairs/100/"+e.flair+".png"}),r=Ti.UI.createView({top:0,width:80,height:Ti.UI.SIZE,layout:"vertical"});return r.add(n),r.add(Ti.UI.createLabel({left:10,width:70,height:Ti.UI.SIZE,color:"#aaa",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,text:"near "+e.city,font:{fontSize:12},_userParentEventListener:!0})),r}function addShareView(e,t,n){var r=_createThumb(e,n),i=Ti.UI.createView({left:5,top:5,width:210,height:Ti.UI.SIZE,layout:"vertical"}),s=Ti.UI.createView({left:0,top:0,width:"200",height:"40",layout:"horizontal"}),o=Ti.UI.createView({left:0,top:7,width:"30",height:"30",backgroundImage:e.photo,borderRadius:2,backgroundColor:"#ccc",_dontUseParentEventListener:!0});s.add(o);var u=Ti.UI.createLabel({height:42.5,left:0,top:0,width:"165",text:e.name,wordWrap:!1,color:"#ccc",font:{fontSize:42},_dontUseParentEventListener:!0});s.add(u);var a=Ti.UI.createView({layout:"horizontal",left:0,width:"200",height:"80",top:0}),f=e.adjective+e.food+" by "+e.recipientname,l=Ti.UI.createLabel({left:0,top:0,height:Ti.UI.SIZE,width:200,color:"#333",_color:"#333",text:f,font:{fontSize:18},_userParentEventListener:!0}),c;e.recipient?c="#2179ca":c="#aaa",i.add(s),i.add(l);var h=Ti.UI.createView({left:0,top:0,height:"40"}),p=require("ui/common/feed/Likes");h.add(p.init(e)),i.add(h);var d=Ti.UI.createView({left:10,right:10,top:10,backgroundColor:"#fff",borderRadius:4,borderWidth:.5,borderColor:"#ddd",height:Ti.UI.SIZE,layout:"horizontal",_data:e,_title:"container",_name_txt:u,_flair_details:a}),v=Ti.UI.createView({left:10,right:10,backgroundColor:"#dedede",borderRadius:4,top:10,width:280,height:80});return d.add(v),t||d.addEventListener("click",function(t){if(!t.source._dontUseParentEventListener==1){var n=require("ui/common/userProfile/UserProfile"),r=require("ui/common/place/Place");portal.open(r.init({vicinity:e.vicinity,lat:e.lat,lng:e.lng,pid:e.pid,name:e.placename,launchRecepient:e.recipient,launchEName:e.recepientname}))}else{var n=require("ui/common/userProfile/UserProfile");portal.open(n.init(e.uid,e.name,e.photo_big))}}),d.add(r),d.add(i),d}var portal=require("ui/common/Portal");exports.feedItem=function(e,t,n){var r;n%2?r="#f4f4f4":r="#eeeeee";var i=Ti.UI.createTableViewRow({height:Ti.UI.SIZE});return i.add(addShareView(e,t,n)),i}