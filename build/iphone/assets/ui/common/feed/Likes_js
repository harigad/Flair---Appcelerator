var portal=require("ui/common/Portal"),like_pofile=require("ui/common/feed/LikeProfile");exports.init=function(e){var t=Ti.UI.createView({layout:"horizontal",width:"120",height:"50",left:0,top:0,bottom:5}),n;Ti.API.debug(e.liked),e.liked?n="images/feed/liked.png":n="images/feed/like_blue.png";var r=Ti.UI.createImageView({image:n,left:0,width:35,bubbleParent:!1});r.addEventListener("click",function(){e.liked?(processLike("delete",e),this.setImage("images/feed/like_blue.png"),e.liked=!1):like_pofile.init(e)}),t.add(r);if(e.likes&&e.likes.length>0){var i=7;e.likes.length<7&&(likes_len=e.likes.length);for(var s=0;s<likes_len;s++){var o=e.likes[0];if(o){var u=o.photo,a=Ti.UI.createView({width:"auto",height:"auto",backgroundImage:"images/feed/like_thumb_1.png",top:0}),f=Ti.UI.createImageView({image:u,width:23,height:23,left:6,top:6,right:6,bottom:6});a.add(f),a.addEventListener("click",function(){var t=require("ui/common/userProfile/UserProfile");portal.open(t.init(e.uid,e.name,e.photo))}),t.add(a)}}}return t}