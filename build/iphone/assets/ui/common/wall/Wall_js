function print(e,t){var n=require("ui/common/feed/FeedView"),r=new n(e);t.add(r)}function loadData(e,t){var n=this,r="http://flair.me/search.php",i={};i.type=e,i.accessToken=Ti.Facebook.getAccessToken();var s=Ti.Network.createHTTPClient({onload:function(n){Ti.API.debug("loaded data for Wall -> "+e);var r=JSON.parse(this.responseText);print(r,t)},onerror:function(t){Ti.API.error("error loading data for Wall -> "+e),Ti.API.error(t.error)},timeout:5e3});s.open("POST",r),s.send(i)}exports.init=function(e){var t;e=="friends"?t="friends":t="near by";var n=Ti.UI.createWindow({title:t,barColor:"#333",backgroundColor:"#eee"});return loadData(e,n),n}