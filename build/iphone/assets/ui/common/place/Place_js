function print(e){var t=Ti.UI.createView({height:"500",layout:"vertical"});main.add(t);var n=Ti.UI.createView({height:"25",width:"100%",left:0,top:0,backgroundColor:"#666"}),r=Ti.UI.createLabel({left:10,top:5,bottom:5,width:"auto",height:"auto",color:"#fff",text:e.city,font:{fontSize:11}});n.add(r),t.add(n),_lat=parseFloat(e.lat),_lng=parseFloat(e.lng);if(!_lat||!_lng)_lat=.02,_lng=.08;if(e.feed){var i=require("ui/common/feed/FeedView"),s=new i(e.feed);t.add(s)}}function print_cast(e){var t=Ti.UI.createView({height:"auto",layout:"vertical"});main.add(t);for(var n=0;n<e.cast.length;n++){var r=Ti.UI.createView({height:"auto",width:"100%",left:0,top:0,layout:"vertical"}),i=Ti.UI.createImageView({image:e.cast[n].photo_big,width:"100%"}),s=Ti.UI.createView({height:"auto",width:"100%",left:0,top:0,backgroundColor:"#eee",layout:"vertical"}),o=Ti.UI.createLabel({left:10,top:10,width:"auto",color:"#999",text:e.cast[n].name,font:{fontSize:16}}),u=Ti.UI.createLabel({left:10,bottom:10,width:"auto",color:"#2179ca",shadowColor:"#aaa",shadowOffset:{x:1,y:1},text:e.cast[n].role_name,font:{fontSize:25}});s.add(o),s.add(u),r.add(i),r.add(s),t.add(r)}}function loadData(e){var t=this;if(e.lat&&e.lng){print(e,sview);return}var n="http://flair.me/search.php",r={};r.type="search",r.searchMode="place",r.pid=e.pid,r.accessToken=Ti.Facebook.getAccessToken();var i=Ti.Network.createHTTPClient({onload:function(t){Ti.API.debug("loaded data for Place -> "+e.pid);var n=JSON.parse(this.responseText);print(n)},onerror:function(t){Ti.API.error("error loading data for Place -> "+e.pid),Ti.API.error(t.error)},timeout:5e3});i.open("POST",n),i.send(r)}var _lat,_lng,portal=require("ui/common/Portal");exports.init=function(e){var t=Ti.UI.createWindow({barColor:"#333",backgroundColor:"#eee"});return loadData(e),t}