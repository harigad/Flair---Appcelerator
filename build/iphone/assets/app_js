Ti.version<1.8&&alert("Sorry - this application template requires Titanium Mobile SDK 1.8 or later"),function(){var e=Ti.Platform.osname,t=Ti.Platform.version,n=Ti.Platform.displayCaps.platformHeight,r=Ti.Platform.displayCaps.platformWidth,i=e==="ipad"||e==="android"&&(r>899||n>899),s;i?s=require("ui/tablet/ApplicationWindow"):e==="android"?s=require("ui/handheld/android/ApplicationWindow"):s=require("ui/handheld/ApplicationWindow"),s.init()}()